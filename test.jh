
entity Machine {
}

entity Users {
	email String
}

// an ignored comment
/** not an ignored comment */
entity MSPD {
	streetAddress String
	postalCode String
	city String
	stateProvince String
}

relationship OneToMany {
	Machine to MSPD
}

relationship ManyToMany {
	Users to Machine
}


// Set pagination options
paginate MSPD with infinite-scroll

